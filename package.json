{
  "name": "napbot-oxy",
  "version": "1.0.0",
  "description": "NapCat-based QQ bot",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "clean": "rimraf dist src/generated/handlebars.templates.js",
    "build": "pnpm build:templates && tsc -p tsconfig.json",
    "build:templates": "handlebars src/templates -f src/generated/handlebars.templates.js -e hbs -r src/templates -c handlebars/runtime",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "oxlint src",
    "dev": "pnpm build:templates && tsx src/main.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.26.2",
  "devDependencies": {
    "@types/node": "^25.0.9",
    "oxlint": "^1.41.0",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "eventsource-parser": "^3.0.6",
    "handlebars": "^4.7.8",
    "zod": "^4.3.5"
  }
}
